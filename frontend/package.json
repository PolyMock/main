{
	"name": "polymock-frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "npm run build && wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2024-01-01 --d1 DB=polymock-db --port=8788 --live-reload",
		"dev:vite": "vite dev",
		"dev:watch": "vite build --watch",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"vite-plugin-node-polyfills": "^0.24.0"
	},
	"dependencies": {
		"@coral-xyz/anchor": "^0.32.1",
		"@magicblock-labs/ephemeral-rollups-sdk": "^0.6.5",
		"@pythnetwork/hermes-client": "^2.0.0",
		"@solana/wallet-adapter-base": "^0.9.27",
		"@solana/wallet-adapter-react": "^0.15.39",
		"@solana/wallet-adapter-react-ui": "^0.9.39",
		"@solana/wallet-adapter-wallets": "^0.19.37",
		"@solana/web3.js": "^1.98.4",
		"axios": "^1.12.2"
	}
}
